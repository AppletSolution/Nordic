<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
            <template id="report_invoice_document_inherit" inherit_id="account.report_invoice_document">
                 <xpath expr="//div[@id='informations']" position="after">
                    <div>
                        <div class="col-auto col-3 mb-2" t-if="o.service_type" name="service_type" style="float:left">
                            <strong>Service Type :</strong>
                            <p class="m-0" t-field="o.service_type"/>
                        </div>
                        <div class="col-auto col-3 mb-2" name="client_booking_no" style="float:left">
                             <strong>Client's Booking No:</strong>
                            <p t-field="o.client_booking_no"/>
                        </div>
                        <div class="col-auto col-3 mb-2" name="nodric_booking_no" style="float:left">
                             <strong>Nordic's Booking No:</strong>
                             <p class="m-0" t-field="o.nodric_booking_no"/>
                        </div>
                        <div class="col-auto col-3 mb-2" t-if="o.cargo_out_date" name="cargo_out_date" style="float:left">
                           <strong>Cargo Out Date:</strong>
                            <p class="m-0" t-field="o.cargo_out_date"/>
                        </div>
                        <div class="col-auto col-3 mb-2" t-if="o.cargo_volume" name="cargo_volume" style="float:left">
                             <strong>Cargo Volume:</strong>
                            <p class="m-0" t-field="o.cargo_volume"/>
                        </div>
                        <div class="col-auto col-3 mb-2" t-if="o.commodity" name="commodity" style="float:left">
                            <strong>Commodity:</strong>
                             <p class="m-0" t-field="o.commodity"/>
                        </div>
                        <div class="col-auto col-3 mb-2" t-if="o.consignee_name" name="consignee_name" style="float:left">
                            <strong>Consignee:</strong>
                             <p class="m-0" t-field="o.consignee_name"/>
                        </div>
                        <div class="col-auto col-3 mb-2" t-if="o.container_no" name="container_no" style="float:left">
                        <strong>Container No:</strong>
                        <p class="m-0" t-field="o.container_no"/>
                        </div>
                        <div class="col-auto col-3 mb-2" t-if="o.departure_location" name="departure_location" style="float:left">
                        <strong>Departure Location:</strong>
                        <p class="m-0" t-field="o.departure_location"/>
                        </div>
                        <div class="col-auto col-3 mb-2" t-if="o.file_upper_run_date" name="file_upper_run_date" style="float:left">
                        <strong>File Upper Run Date:</strong>
                        <p class="m-0" t-field="o.file_upper_run_date"/>
                        </div>
                        <div class="col-auto col-3 mb-2" t-if="o.final_destination" name="final_destination" style="float:left">
                        <strong>Final Destination:</strong>
                        <p class="m-0" t-field="o.final_destination"/>
                        </div>
                        <div class="col-auto col-3 mb-2" t-if="o.job_no" name="job_no" style="float:left">
                        <strong>Job No:</strong>
                        <p class="m-0" t-field="o.job_no"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.id_ed_no" name="id_ed_no" style="float:left">
                        <strong>ID/ED No:</strong>
                        <p class="m-0" t-field="o.id_ed_no"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.no_of_id_ed" name="no_of_id_ed" style="float:left">
                        <strong>No of ID/ED:</strong>
                        <p class="m-0" t-field="o.no_of_id_ed"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.pickup_location" name="pickup_location" style="float:left">
                        <strong>Pickup Location:</strong>
                        <p class="m-0" t-field="o.pickup_location"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.port_exam_date" name="port_exam_date" style="float:left">
                        <strong>Port Exam Date:</strong>
                        <p class="m-0" t-field="o.port_exam_date"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.shipment_date" name="shipment_date" style="float:left">
                        <strong>Shipment Date:</strong>
                        <p class="m-0" t-field="o.shipment_date"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.shipment_name" name="shipment_name" style="float:left">
                        <strong>Shipment Name:</strong>
                        <p class="m-0" t-field="o.shipment_name"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.shipment_line_name" name="shipment_line_name" style="float:left">
                        <strong>Shipment line Name:</strong>
                        <p class="m-0" t-field="o.shipment_line_name"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.transaction_date" name="transaction_date" style="float:left">
                        <strong>Transaction Date:</strong>
                        <p class="m-0" t-field="o.transaction_date"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.track_no_mm" name="track_no_mm" style="float:left">
                        <strong>Track No (Myanmar):</strong>
                        <p class="m-0" t-field="o.track_no_mm"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.track_no_th" name="track_no_th" style="float:left">
                        <strong>Track No (Thailand):</strong>
                        <p class="m-0" t-field="o.track_no_th"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.track_type" name="track_type" style="float:left">
                        <strong>Track Type:</strong>
                        <p class="m-0" t-field="o.track_type"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.vessel_no" name="vessel_no" style="float:left">
                        <strong>Vessel No:</strong>
                        <p class="m-0" t-field="o.vessel_no"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.warehouse_id" name="warehouse_id" style="float:left">
                        <strong>Warehouse:</strong>
                        <p class="m-0" t-field="o.warehouse_id"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.eta_ygn_date" name="eta_ygn_date" style="float:left">
                        <strong>ETA YGN:</strong>
                        <p class="m-0" t-field="o.eta_ygn_date"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.etd_ygn_date" name="etd_ygn_date" style="float:left">
                        <strong>ETD YGN:</strong>
                        <p class="m-0" t-field="o.etd_ygn_date"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.eta_mwd_date" name="eta_mwd_date" style="float:left">
                        <strong>ETA MWD:</strong>
                        <p class="m-0" t-field="o.eta_mwd_date"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.etd_mwd_date" name="etd_mwd_date" style="float:left">
                        <strong>ETD MWD:</strong>
                        <p class="m-0" t-field="o.etd_mwd_date"/>
                        </div>
                        <div class="col-auto col-3 mb-2" t-if="o.eta_lcb_date" name="eta_lcb_date" style="float:left">
                        <strong>ETA LCB:</strong>
                        <p class="m-0" t-field="o.eta_lcb_date"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.etd_lcb_date" name="etd_lcb_date" style="float:left">
                        <strong>ETD LCB:</strong>
                        <p class="m-0" t-field="o.etd_lcb_date"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.eta_sva_date" name="eta_sva_date" style="float:left">
                        <strong>ETA SVA:</strong>
                        <p class="m-0" t-field="o.eta_sva_date"/>
                        </div>

                        <div class="col-auto col-3 mb-2" t-if="o.etd_sva_date" name="etd_sva_date" style="float:left">
                        <strong>ETD SVA:</strong>
                        <p class="m-0" t-field="o.etd_sva_date"/>
                        </div>
                    </div>

                 </xpath>
            </template>
    </data>
</odoo>